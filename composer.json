{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.10.2.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "2.3.2",
                "name": "twitter/bootstrap-zip",
                "dist": {
                    "url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "2.0.6",
                "name": "sma-spot",
                "dist": {
                    "url": "https://sma-spot.googlecode.com/files/SMAspot_SRC_206_Linux_Win32.tar",
                    "type": "tar"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "3.0.2",
                "name": "highcharts",
                "dist": {
                    "url": "http://www.highcharts.com/downloads/zips/Highcharts-3.0.2.zip",
                    "type": "zip"
                }
            }
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "silex/silex": "1.1.*",
        "igorw/config-service-provider": "1.2.*",
        "silex/web-profiler": "1.0.*",
        "symfony/browser-kit": "~2.2",
        "symfony/class-loader": "~2.2",
        "symfony/config": "~2.2",
        "symfony/console": "~2.2",
        "symfony/css-selector": "~2.2",
        "symfony/dependency-injection": "~2.2",
        "symfony/finder": "~2.2",
        "symfony/form": "~2.2",
        "symfony/monolog-bridge": "~2.2",
        "symfony/process": "~2.2",
        "symfony/security": "~2.2",
        "symfony/translation": "~2.2",
        "symfony/twig-bridge": "~2.2",
        "symfony/validator": "~2.2",
        "symfony/yaml": "~2.2",
        "kilte/silex-pagination": "~1.0",
        "doctrine/dbal": "~2.5.0@dev",
        "ivaynberg/select2": "dev-master",
        "jquery/jquery": "*",
        "highcharts": "*",
        "twitter/bootstrap-zip": "*",
        "sma-spot": "*"
    },

    "scripts": {
        "post-update-cmd": "Composerscript::postUpdate",
        "post-install-cmd": "Composerscript::postInstall"
    },

    "autoload": {
        "psr-0": { "": "src/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
